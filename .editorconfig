
# .editorconfig
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.yml]
indent_size = 2
[*.yaml]
indent_size = 2

[*.json]
indent_size = 2

[*.ps1]
indent_size = 2
[*.psm1]
indent_size = 2
[*.psd1]
indent_size = 2
